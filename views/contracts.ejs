<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Contracts</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="/css/contracts.css">
    <link rel="icon" type="image/png" href="img/logo.png" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/v/bs4/dt-1.12.1/sp-2.0.1/sl-1.4.0/datatables.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/plug-ins/1.10.19/sorting/datetime-moment.js" type="text/javascript"></script>
    <script src="/js/contracts.js"></script>

</head>

<body>


    <nav>
        <ul>
            <li value="1">
                Summary/Outstanding
            </li>
            <li value="2">
                In-Progress
            </li>
            <li value="3">
                Finished
            </li>
            <li value="4">
                Deleted/Cancelled/Rejected
            </li>
        </ul>
    </nav>

    <% var current = new Date(); %>
    <div class="main">

        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="1">
            <h2>summary by route</h2>
            <table id="summary" class="dataTable">
                <tr>
                    <th>S.No.</th>
                    <th>Contracts</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <th>Average Wait</th>
                    <th>Max Wait</th>
                </tr>
                <% let sno = 1  %>
                <% routes.forEach((route)=>{%>
                <tr>
                    <td><%=sno%></td>
                    <td><%=route.count%></td>
                    <td class="start"><%=route.from%></td>
                    <td><%=route.to%></td>
                    <td><%=parseInt(route.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> m<sup>3</sup></td>
                    <td><%=parseInt(route.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <td><%=parseInt(route.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <%let epoch_time = (route.totalTime/route.count);%>
                    <% var date_obj = new Date(epoch_time);%>
                    <%let hrs = date_obj.getHours();%>
                    <% let mins = date_obj.getMinutes();%>
                    <% let hhmm = (hrs < 10 ? "0" + hrs : hrs) + " hours " + (mins < 10 ? "0" + mins : mins) +" minutes";%>
                    <td> <%=hhmm%></td>
                    <%epoch_time = (route.maxTime);%>
                    <%  date_obj = new Date(epoch_time);%>
                    <% hrs = date_obj.getHours();%>
                    <%  mins = date_obj.getMinutes();%>
                    <% hhmm = (hrs < 10 ? "0" + hrs : hrs) + " hours " + (mins < 10 ? "0" + mins : mins) +" minutes";%>
                    <td><%=hhmm%></td>
                </tr>
                <% sno += 1; %>
                <%})%>
            </table>
        </div>
        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="1">

            <h2>rush</h2>

            <table id="rush" class="dataTable">
                <tr>
                    <th>S.No.</th>
                    <th>Client</th>
                    <th style="width: 150px;">Date Created</th>
                    <th style="width: 150px;">Time Passed</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Jumps</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <th>Key</th>
                    <th style="width: 150px;">Delta (Reward : Collateral : Volume)</th>
                </tr>
                <%  sno = 1  %>
                <% contracts.forEach((contract)=>{%>
                <% if(contract.description.split("-")[1] == "R" && contract.status == "outstanding"){%>
                <% let noCode = false %>

                <% if(!contract.appraisalReward){ contract.appraisalReward = Infinity; noCode=true} %>
                <% if(!contract.appraisalCollateral){ contract.appraisalCollateral = Infinity; noCode=true} %>
                <% if(!contract.appraisalVolume){contract.appraisalVolume = Infinity; noCode=true} %>
                <% let rewardDelta = contract.reward/contract.appraisalReward %>
                <% let collateralDelta = contract.collateral/contract.appraisalCollateral %>
                <% let volumeDelta = contract.volume/contract.appraisalVolume %>
                <% let isRouteInvalid =  contract.appraisalService == "Standard Routes" && !(contract.start.includes(contract.appraisalFrom.substring(0, contract.appraisalFrom.indexOf(' '))) && contract.end.includes(contract.appraisalTo.substring(0, contract.appraisalTo.indexOf(' ')))) %>


                <% if(!noCode && rewardDelta >= 0.9 && collateralDelta >= 0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05 && !isRouteInvalid){ %>
                <tr style="background-color: rgba(144, 255, 144, 0.5);">
                    <%}%>
        <% if((!noCode && !(rewardDelta >= 0.9 && collateralDelta >=0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05)) || isRouteInvalid){ %>
                <tr style="background-color: rgba(255, 113, 113, 0.5);">
                    <%}%>
        <% if(noCode){ %>
                <tr style="background-color: rgba(255, 191, 0, 0.15);">
                    <%}%>
                    <td><%=sno%></td>
                    <%var options = {  hour:"numeric", minute:"numeric" , month: 'long', day: 'numeric', }; %>
                    <% let date  = new Date(contract.date);%>
                    <%var hours = Math.floor(Math.abs(current - date) / 36e5);%>
                    <%var minutes = Math.floor(Math.abs(current - date) % 36e5 / 60000);%>
                    <td><%=contract.issuerName%></td>

                    <td><%=date.toLocaleDateString("en-GB", options)%></td>
                    <% if(hours<1){%>
                    <td><%=minutes.toFixed(0)%> minutes</td>
                    <%}%>
            <% if(hours>=1){%>
                    <td><%=hours.toFixed(0)%> hours <%=(minutes%60).toFixed(0)%> minutes</td>
                    <%}%>
                    <td class="start"><%=contract.start%></td>
                    <td><%=contract.end%></td>
                    <td><%=contract.appraisalJumps%></td>
                    <td><%=parseInt(contract.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> m<sup>3</sup>
                    </td>
                    <td><%=parseInt(contract.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <td><%=parseInt(contract.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <td><%=contract.key%></td>
                    <%if(isRouteInvalid){%>
                    <td>Incorrect Route</td>
                    <%}%>
                        <%if(!isRouteInvalid){%>
                    <td><%=rewardDelta.toFixed(2)%> : <%=collateralDelta.toFixed(2)%> : <%=volumeDelta.toFixed(2)%></td>
                    <%}%>

                </tr>
                <% sno += 1; %>
                    <%}})%>
            </table>
        </div>
        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="1">

            <h2>outstanding</h2>

            <table id="outstanding" class="dataTable">
              <thead>
                <tr>
                    <th>S.No.</th>
                    <th>Client</th>
                    <th style="width: 150px;">Date Created</th>
                    <th style="width: 150px;">Time Passed</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Jumps</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <Th>Rush</Th>
                    <th>Service</th>
                    <th>Key</th>
                    <th style="width: 150px;">Delta (Reward : Collateral : Volume)</th>
                    <th>Mailed</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>Sum of Selected:</th>
                  <th colspan="6"></th>
                  <th class="outstandingVolumeSum"> m<sup>3</sup></th>
                  <th class="outstandingRewardSum" > ISK</th>
                  <th class="outstandingCollateralSum"> ISK</th>
                  <th colspan="5"></th>
                </tr>
              </tfoot>
                <%  sno = 1  %>
                <% contracts.forEach((contract)=>{%>
                <% if(contract.status == "outstanding"){%>
                <% let noCode = false %>

                <% if(!contract.appraisalReward){ contract.appraisalReward = Infinity; noCode=true} %>
                <% if(!contract.appraisalCollateral){ contract.appraisalCollateral = Infinity; noCode=true} %>
                <% if(!contract.appraisalVolume){contract.appraisalVolume = Infinity; noCode=true} %>
                <% let rewardDelta = contract.reward/contract.appraisalReward %>
                <% let collateralDelta = contract.collateral/contract.appraisalCollateral %>
                <% let volumeDelta = contract.volume/contract.appraisalVolume %>


                <% let isRouteInvalid =  contract.appraisalService == "Standard Routes" && !(contract.start.includes(contract.appraisalFrom.substring(0, contract.appraisalFrom.indexOf(' '))) && contract.end.includes(contract.appraisalTo.substring(0, contract.appraisalTo.indexOf(' ')))) %>


                <% if(!noCode && rewardDelta >= 0.9 && collateralDelta >= 0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05 && !isRouteInvalid){ %>
                <tr style="background-color: rgba(5, 192, 1, 0.75);">
                    <%}%>
                <% if((!noCode && !(rewardDelta >= 0.9 && collateralDelta >=0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05)) || isRouteInvalid){ %>
                <tr style="background-color: rgba(255, 60, 60, 0.5);">
                    <%}%>
                <% if(noCode && (contract.secondaryValidationReward) < 0){ %>
                <tr style="background-color: rgba(255, 191, 0, 0.15);">
                    <%}%>
                <% if(noCode && contract.secondaryValidationReward >= 0.9) { %>
                <tr style="background-color: rgba(144, 255, 144, 0.5);">
                    <%}%>
                <% if(noCode && (contract.secondaryValidationReward < 0.9 && contract.secondaryValidationReward >= 0)) { %>
                <tr style="background-color: rgba(255, 130, 130, 0.5);">
                    <%}%>
            <td><%=sno%></td>
                    <td><%=contract.issuerName%></td>

                    <%var options = {  hour:"numeric", minute:"numeric" , month: 'long', day: 'numeric', }; %>
                    <% let date  = new Date(contract.date);%>
                    <%var hours = Math.floor(Math.abs(current - date) / 36e5);%>
                    <%var minutes = Math.floor(Math.abs(current - date) % 36e5 / 60000);%>

                    <td><%=date.toLocaleDateString("en-GB", options)%></td>
                    <% if(hours<1){%>
                    <td><%=minutes.toFixed(0)%> minutes</td>
                    <%}%>
            <% if(hours>=1){%>
                    <td><%=hours.toFixed(0)%> hours <%=(minutes%60).toFixed(0)%> minutes</td>
                    <%}%>
            <td class="start"><%=contract.start%></td>
                    <td><%=contract.end%></td>
                    <td><%=contract.appraisalJumps%></td>
                    <td><%=parseInt(contract.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        m<sup>3</sup></td>
                    <td><%=parseInt(contract.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <td><%=parseInt(contract.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <%if(contract.description.split("-")[1]==="R"){%>
                    <td>Yes</td>
                    <%}%>
                    <%if(contract.description.split("-")[1]!="R"){%>
                    <td>No</td>
                    <%}%>
                    <td><%=contract.service%></td>
                    <%if(noCode && (contract.secondaryValidationReward) < 0){%>
                      <td>No Route Found</td>
                      <%}
                    if(noCode && (contract.secondaryValidationReward) >= 0){%>
                        <td>Validation Attempted</td>
                      <%}
                      if(!noCode){%>
                      <td><%=contract.key%></td>
                      <%}%>
                    <%if(isRouteInvalid){%>
                    <td>Incorrect Route</td>
                    <%}%>
                    <%if(noCode){%>
                    <td><%=contract.secondaryValidationReward.toFixed(2)%> : <%=contract.secondaryValidationReward.toFixed(2)%> : <%=contract.secondaryValidationReward.toFixed(2)%></td>
                    <%}%>
                    <%if(!isRouteInvalid && !noCode){%>
                    <td><%=rewardDelta.toFixed(2)%> : <%=collateralDelta.toFixed(2)%> : <%=volumeDelta.toFixed(2)%></td>
                    <%}%>
                    <td><%=contract.mailed%></td>


                </tr>
                <% sno += 1; %>
                <%}})%>
            </table>
        </div>
        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="2">

            <h2>in progress</h2>
            <table id="inprogress" class="dataTable">
              <thead>
                <tr>
                    <th>S.No.</th>
                    <th>Client</th>
                    <th style="width: 150px;">Date Created</th>
                    <th style="width: 150px;">Time Passed</th>
                    <th>Pilot Name</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <th>Service</th>
                    <th>Key</th>
                    <th style="width: 150px;">Delta (Reward : Collateral : Volume)</th>
                </tr>
                <thead>
                <% sno = 1; %>
                <% contracts.forEach((contract)=>{%>
                <% if(contract.status == "in_progress"){%>
                <% let noCode = false %>

                <% if(!contract.appraisalReward){ contract.appraisalReward = Infinity; noCode=true} %>
                <% if(!contract.appraisalCollateral){ contract.appraisalCollateral = Infinity; noCode=true} %>
                <% if(!contract.appraisalVolume){contract.appraisalVolume = Infinity; noCode=true} %>
                <% let rewardDelta = contract.reward/contract.appraisalReward %>
                <% let collateralDelta = contract.collateral/contract.appraisalCollateral %>
                <% let volumeDelta = contract.volume/contract.appraisalVolume %>


                <% if(!noCode && rewardDelta >= 0.9 && collateralDelta >= 0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05){ %>
                <tr style="background-color: rgba(144, 255, 144, 0.5);">
                    <%}%>
        <% if(!noCode && !(rewardDelta >= 0.9 && collateralDelta >=0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05)){ %>
                <tr style="background-color: rgba(255, 113, 113, 0.5);">
                    <%}%>
        <% if(noCode){ %>
                <tr style="background-color: rgba(255, 191, 0, 0.15);">
                    <%}%>
            <td><%=sno%></td>
                    <td><%=contract.issuerName%></td>

                    <%var options = {  hour:"numeric", minute:"numeric" , month: 'long', day: 'numeric', }; %>
                    <% let date  = new Date(contract.date);%>
                    <%var hours = Math.floor(Math.abs(current - date) / 36e5);%>
                    <%var minutes = Math.floor(Math.abs(current - date) % 36e5 / 60000);%>

                    <td><%=date.toLocaleDateString("en-GB", options)%></td>
                    <% if(hours<1){%>
                    <td><%=minutes.toFixed(0)%> minutes</td>
                    <%}%>
            <% if(hours>=1){%>
                    <td><%=hours.toFixed(0)%> hours <%=(minutes%60).toFixed(0)%> minutes</td>
                    <%}%>
                    <td><%=contract.acceptorName%></td>
            <td class="start"><%=contract.start%></td>
                    <td><%=contract.end%></td>
                    <td><%=parseInt(contract.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        m<sup>3</sup></td>
                    <td><%=parseInt(contract.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK</td>
                    <td><%=parseInt(contract.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK
                    </td>
                    <td><%=contract.service%></td>
                    <td><%=contract.key%></td>
                    <td><%=rewardDelta.toFixed(2)%> : <%=collateralDelta.toFixed(2)%> :
                        <%=volumeDelta.toFixed(2)%></td>

                </tr>
                <% sno += 1; %>
                <%}})%>
            </table>
        </div>
        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="3">

            <h2>finished</h2>
            <table id="finished" class="dataTable">
              <thead>
                <tr>
                    <th>S.No.</th>
                    <th>Client</th>
                    <th>Created At</th>
                    <th>Finished At</th>
                    <th>Pilot Name</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <th>Service</th>
                    <th>Key</th>
                    <th style="width: 150px;">Delta (Reward : Collateral : Volume)</th>
                </tr>
              </thead>
                <% sno = 1; %>
                <% contracts.forEach((contract)=>{%>
                <% if(contract.status == "finished"){%>
                <% let noCode= false;%>
                <% if(!contract.appraisalReward){ contract.appraisalReward = Infinity; noCode=true} %>
                <% if(!contract.appraisalCollateral){ contract.appraisalCollateral = Infinity; noCode=true} %>
                <% if(!contract.appraisalVolume){contract.appraisalVolume = Infinity; noCode=true} %>
                <% let rewardDelta = contract.reward/contract.appraisalReward %>
                <% let collateralDelta = contract.collateral/contract.appraisalCollateral %>
                <% let volumeDelta = contract.volume/contract.appraisalVolume %>


                <% if(!noCode && rewardDelta >= 0.9 && collateralDelta >= 0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05){ %>
                <tr style="background-color: rgba(144, 255, 144, 0.5);">
                    <%}%>
        <% if(!noCode && !(rewardDelta >= 0.9 && collateralDelta >=0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05)){ %>
                <tr style="background-color: rgba(255, 113, 113, 0.5);">
                    <%}%>
        <% if(noCode){ %>
                <tr style="background-color: rgba(255, 191, 0, 0.15);">
                    <%}%>
                    <td><%=sno%></td>
                    <td><%=contract.issuerName%></td>
                    <%var options = {  hour:"numeric", minute:"numeric" , month: 'long', day: 'numeric', }; %>
                    <% let date  = new Date(contract.date);%>
                    <% let dateCompleted  = new Date(contract.dateCompleted);%>

                    <td><%=date.toLocaleDateString("en-GB", options)%></td>
                    <td><%=dateCompleted.toLocaleDateString("en-GB", options)%></td>

                    <td><%=contract.acceptorName%></td>
                    <td class="start"><%=contract.start%></td>
                    <td><%=contract.end%></td>
                    <td><%=parseInt(contract.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        m<sup>3</sup></td>
                    <td><%=parseInt(contract.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%> ISK
                    </td>
                    <td><%=parseInt(contract.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        ISK</td>
                    <td><%=contract.service%></td>
                    <td><%=contract.key%></td>
                    <td><%=rewardDelta.toFixed(2)%> : <%=collateralDelta.toFixed(2)%> : <%=volumeDelta.toFixed(2)%></td>

                </tr>
                <% sno += 1; %>
                <%}})%>
            </table>
        </div>
        <div class="table-wrapper dataTables_wrapper container-fluid dt-bootstrap4" value="4">

            <h2>Cancelled/Failed/Rejected/Deleted</h2>
            <table id="cancelled" class="dataTable">
                <% sno = 1 %>
                <thead>
                <tr>
                    <th>S.No.</th>
                    <th>Client</th>
                    <th>Created At</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Volume</th>
                    <th>Reward</th>
                    <th>Collateral</th>
                    <th>Service</th>
                    <th>Key</th>
                    <th style="width: 150px;">Delta (Reward : Collateral : Volume)</th>
                    <th>Status</th>
                </tr>
              </thead>
                <% sno = 1; %>
                <% contracts.forEach((contract)=>{%>
                <% if(contract.status == "cancelled" || contract.status == "failed" || contract.status == "rejected" || contract.status == "deleted"){%>
                <% let noCode=false%>
                <% if(!contract.appraisalReward){ contract.appraisalReward = Infinity; noCode=true} %>
                <% if(!contract.appraisalCollateral){ contract.appraisalCollateral = Infinity; noCode=true} %>
                <% if(!contract.appraisalVolume){contract.appraisalVolume = Infinity; noCode=true} %>
                <% let rewardDelta = contract.reward/contract.appraisalReward %>
                <% let collateralDelta = contract.collateral/contract.appraisalCollateral %>
                <% let volumeDelta = contract.volume/contract.appraisalVolume %>


                <% if(!noCode && rewardDelta >= 0.9 && collateralDelta >= 0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05){ %>
                <tr style="background-color: rgba(144, 255, 144, 0.5);">
                    <%}%>
        <% if(!noCode && !(rewardDelta >= 0.9 && collateralDelta >=0.9 && volumeDelta >=0.95 && volumeDelta <= 1.05)){ %>
                <tr style="background-color: rgba(255, 113, 113, 0.5);">
                    <%}%>
        <% if(noCode){ %>
                <tr style="background-color: rgba(255, 191, 0, 0.15);">
                    <%}%>
            <td><%=sno%></td>
                    <td><%=contract.issuerName%></td>
                    <%var options = {  hour:"numeric", minute:"numeric" , month: 'long', day: 'numeric', }; %>
                    <% let date  = new Date(contract.date);%>
                    <% let dateCompleted  = new Date(contract.dateCompleted);%>

                    <td><%=date.toLocaleDateString("en-GB", options)%></td>
                    <td class="start"><%=contract.start%></td>
                    <td><%=contract.end%></td>
                    <td><%=parseInt(contract.volume).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        m<sup>3</sup></td>
                    <td><%=parseInt(contract.reward).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        ISK</td>
                    <td><%=parseInt(contract.collateral).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>
                        ISK</td>
                    <td><%=contract.service%></td>
                    <td><%=contract.key%></td>
                    <td><%=rewardDelta.toFixed(2)%> : <%=collateralDelta.toFixed(2)%> :
                        <%=volumeDelta.toFixed(2)%></td>
                    <td><%=contract.status%></td>
                </tr>
                <% sno += 1; %>
                <%}})%>
            </table>

        </div>

</body>

</html>
